<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="line" >

    <!-- 显示虚线，破折线的宽度为dashWith，空隙的宽度为dashGap, darkgray -->
    <stroke
        android:dashGap="5dp"
        android:dashWidth="1dp"
        android:width="10dp"
        android:color="#A9A9A9" />

    <size android:height="1dp" />

</shape>